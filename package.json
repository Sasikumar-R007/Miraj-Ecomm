{
  "name": "miraj-candles-workspace",
  "version": "1.0.0",
  "description": "Miraj Candles E-commerce Platform",
  "scripts": {
    "dev:client": "cd client && npm run dev",
    "dev:server": "cd server && npm run dev",
    "dev:admin": "cd admin && npm run dev",
    "build:client": "cd client && npm run build",
    "build:admin": "cd admin && npm run build",
    "install:all": "cd client && npm install && cd ../server && npm install && cd ../admin && npm install"
  },
  "workspaces": [
    "client",
    "server",
    "admin"
  ],
  "dependencies": {
    "react-toastify": "^11.0.5"
  }
}
